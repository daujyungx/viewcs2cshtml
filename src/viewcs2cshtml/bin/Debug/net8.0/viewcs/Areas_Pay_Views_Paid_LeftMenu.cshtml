@{
Layout = null;
    _ = (WebConfig)ViewBag.MyWebConfig;
    vAgentUser LoginAgentUser = ViewBag.LoginAgentUser;
    _ = BaseService.MyProductService;
    IAgentUserService MyAgentUserService = BaseService.MyAgentUserService;
}

    <div class="portlet-title">
        <div class="caption caption-md">
            <i class="fa fa-search font-green"></i>
            <span class="caption-subject font-green bold uppercase">搜索</span>
        </div>
    </div>
    <div class="portlet-body">

@if(!MyAgentUserService.CheckUserPower(LoginAgentUser, AgentEnum.UserRoleGroup.客服))
{
            <form class="form search-filter" id="searchForm" action="/Finance/Pay/Paid" method="GET">


    
                <input type="hidden" name="MoneyStateId" value="@(Model.MoneyStateId)" />
                <div class="form-group">
                    <label class="control-label bold">订单编号</label>
                    <div class="controls">
                        <input name="OrderNo" value="@(Model.OrderNo)" type="text" placeholder="请输入订单编号" class="form-control" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label bold">付款时间</label>
                    <div class="controls">
                        <input class="form-control dateISO" onclick="WdatePicker({ doubleCalendar: true, dateFmt: 'yyyy-MM-dd' })" type="text" name="PayableDate" value="@(Model.PayableDate)" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label bold">金 额</label>
                    <div class="controls">
                        <input name="Money" value="@(Model.Money)" class="form-control" type="text" placeholder="请输入订单金额" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label bold">账款类型</label>
                    <div class="controls">
                        <select class="form-control" id="MoneyTypeId" name="MoneyTypeId">
                            <option value="0">


    选择类型
}
@*__tagHelperAttribute_0*@
</option>

@foreach (KeyValuePair<string, string> type in EnumHelper<OrderEnum.MoneyTypeCategory>.GetDict())
				{
                                <option value='@(type.Key)" @((type.Key == Model.MoneyTypeId.ToString()) ? "selected='selected'" : "")>@(type.Value)</option>

}
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label bold">供应商</label>
                    <label class="pull-right">
                    </label>
                    <div class="controls">
                        <input name="SupplierName" value="@(Model.SupplierName)" type="text" placeholder="请输入供应商名称" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label bold">大巴供应商</label>
                    <label class="pull-right">
                    </label>
                    <div class="controls">
                        <input name="TrafficSupplierName" value="@(Model.TrafficSupplierName)" type="text" placeholder="请输入供应商名称" class="form-control" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label bold">航班日期</label>
                    <div class="controls">
                        <input class="form-control dateISO" onclick="WdatePicker({ doubleCalendar: true, dateFmt: 'yyyy-MM-dd' })" type="text" name="CruiseDate" value="@(Model.CruiseDate)" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label bold">邮轮</label>
                    <div class="controls">
                        <select class="form-control" id="ShipId" name="ShipId">
                            <option value="0">


    选择邮轮
}
@*__tagHelperAttribute_0*@
</option>

@foreach (vShip ship in BaseService.MyShipService.GetPublicProductShips())
				{
                                <option value='@(ship.ShipId)" @((ship.ShipId == Model.ShipId) ? "selected='selected'" : "")>@(ship.ShipName)</option>

}
                        </select>

                    </div>
                </div>

                <button type="submit" class="btn green btn-block"><i class="fa fa-search"></i>搜索</button>
            
}
<option value="{optionvaluename}">


    选择类型
}
<option value="{optionvaluename}">


    选择邮轮
}
</form></form></form></form>

}
    </div>


